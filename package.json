{
	"name": "sveltekit-content-security-policy",
	"version": "0.0.1",
	"license": "BSD-3-Clause",
	"description": "SvelteKit Content Security Policy: how you can add CSP to reduce your Svelte site's cross-site scripting (XSS) attack surface.",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run generate:headers",
		"preview": "vite preview",
		"check": "svelte-check",
		"check:watch": "svelte-check --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"lint:scss": "stylelint \"src/**/*.{css,scss,svelte}\"",
		"format": "prettier --write --plugin-search-dir=. .",
		"generate:headers": "node ./generate-headers.js",
		"generate:images": "node ./generate-responsive-image-data.js",
		"generate:manifest": "node ./generate-manifest.js",
		"generate:sitemap": "node ./generate-sitemap.js",
		"prettier:check": "prettier --check --plugin-search-dir=. .",
		"prepare": "husky install"
	},
	"devDependencies": {
		"@commitlint/config-conventional": "^17.6.3",
		"@fontsource/lato": "^4.5.10",
		"@fontsource/lora": "^4.5.13",
		"@fontsource/source-sans-pro": "^4.5.11",
		"@iconify/svelte": "^3.1.3",
		"@rodneylab/sveltekit-components": "2.0.2",
		"@sveltejs/adapter-static": "2.0.2",
		"@sveltejs/kit": "^1.18.0",
		"@types/object-hash": "^3.0.2",
		"commitlint": "^17.6.3",
		"dayjs": "^1.11.7",
		"dotenv": "^16.0.3",
		"eslint": "^8.40.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"front-matter": "^4.0.2",
		"husky": "^8.0.3",
		"leaflet": "^1.9.4",
		"mdsvex": "^0.10.6",
		"node-html-parser": "6.1.5",
		"object-hash": "^3.0.0",
		"postcss": "^8.4.23",
		"postcss-html": "^1.5.0",
		"prettier": "~2.8.8",
		"prettier-plugin-svelte": "^2.10.0",
		"reading-time": "1.4.0",
		"sass": "^1.62.1",
		"sharp": "^0.32.1",
		"stylelint": "^15.6.2",
		"stylelint-config-recommended-scss": "^11.0.0",
		"stylelint-config-standard": "^33.0.0",
		"stylelint-scss": "^5.0.0",
		"svelte": "^3.59.1",
		"svelte-check": "^3.3.2",
		"svelte-preprocess": "^5.0.3",
		"vanilla-lazyload": "^17.8.3",
		"vite": "^4.3.8",
		"vite-imagetools": "^5.0.4"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/rodneylab/sveltekit-content-security-policy"
	},
	"bugs": {
		"url": "https://github.com/rodneylab/sveltekit-content-security-policy/issues"
	},
	"type": "module"
}
